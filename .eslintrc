{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "import/prefer-default-export": "off",
    "linebreak-style": 0,
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ],
    "no-unused-vars": 0
  }
}
